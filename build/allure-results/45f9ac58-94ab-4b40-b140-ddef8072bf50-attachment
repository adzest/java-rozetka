(function () {
var status_orders = $('status_orders');
if (status_orders) {
$('status_orders').addEvent('click', function(e) {
new Event(e).stop();
document.fireEvent('trackOrder');
var account_href = 'https://my.rozetka.com.ua/profile/account/#details';
if (User.isAuth()) {
window.location = account_href;
} else {
var popup = User.getPopup();
if (popup.isOpened()) {
popup.close();
} else {
document.addEvent('signIn', function() {
window.location = account_href;
});
User.SignIn();
}
}
});
}
})();
