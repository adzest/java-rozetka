<script>
(function(){
var isCheaperThenItWas = function (price_json) {
return price_json.old_price &amp;&amp; Math.round(price_json.old_price) &gt; Math.round(price_json.price);
};
var redrawOldPriceLabel = function (price_json) {
var template = "{if old_price &amp;&amp; old_price_formatted &amp;&amp; Math.round(old_price) &gt; Math.round(price)} &lt;div name=\"price\" class=\"detail-price-old\"&gt; &lt;div class=\"detail-price-uah-old\"&gt;${old_price_formatted}&lt;span&gt;&amp;thinsp;${old_price_unit}&lt;\/span&gt;&lt;\/div&gt; &lt;\/div&gt; {\/if}",
old_price_label = document.getElementById('old_price_label'),
price_container = document.getElementById('price_container'),
cheaper_price_class = 'detail-price-cheaper',
content = '';
if (old_price_label &amp;&amp; Object.keys(price_json).length &amp;&amp; isCheaperThenItWas(price_json)) {
content = App.getHTML(template, price_json);
old_price_label.innerHTML = content;
}
if (price_container) {
price_container.removeClass(cheaper_price_class);
if (content &amp;&amp; !price_container.hasClass(cheaper_price_class)) {
price_container.addClass(cheaper_price_class);
}
}
};
App.addEvent('redrawOldPriceLabel', redrawOldPriceLabel);
price_json.old_price_unit = 'грн';
App.addOnDomReady(function () {
redrawOldPriceLabel(price_json);
});
}());
</script>
