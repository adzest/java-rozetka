var popup = new Popup_class('&lt;div class=\"popup-css payment-methods-popup\"&gt; &lt;a href=\"#\" class=\"popup-close\" name=\"close\"&gt;&lt;img width=\"27\" height=\"27\" src=\"https:\/\/i.rozetka.ua\/design\/_.gif\" alt=\"×\" class=\"popup-close-medium-icon sprite\"&gt;&lt;\/a&gt; &lt;div class=\"payment-methods-description-wrap\"&gt; &lt;div class=\"payment-methods-description\" name=\"content\"&gt;&lt;\/div&gt; &lt;\/div&gt; &lt;\/div&gt;');
